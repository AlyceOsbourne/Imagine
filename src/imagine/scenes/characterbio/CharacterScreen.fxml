<fx:root dividerPositions="0.25" orientation="VERTICAL" prefHeight="768.0" prefWidth="1024.0" type="SplitPane"
         xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <Label alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="true" prefHeight="188.0" prefWidth="1022.4"
          text="Character Biographies">
      <font>
         <Font name="Blackadder ITC" size="96.0"/>
      </font>
   </Label>
   <AnchorPane SplitPane.resizableWithParent="false">
      <SplitPane dividerPositions="0.25" prefHeight="572.8" prefWidth="1022.4" scaleShape="false"
                 AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                 AnchorPane.topAnchor="0.0">
         <TreeView fx:id="characterlibrary" SplitPane.resizableWithParent="false"/>
         <SplitPane dividerPositions="0.75" orientation="VERTICAL" prefHeight="571.2" prefWidth="763.2"
                    SplitPane.resizableWithParent="false">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0"
                        SplitPane.resizableWithParent="false">
               <HBox layoutX="281.0" layoutY="162.0" prefHeight="424.0" prefWidth="762.4"
                     AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                     AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                  <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
                     <TextField fx:id="title" alignment="CENTER">
                        <VBox.margin>
                           <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
                        </VBox.margin>
                     </TextField>
                     <TextField fx:id="forename" alignment="CENTER">
                        <VBox.margin>
                           <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
                        </VBox.margin>
                     </TextField>
                     <TextField fx:id="middlename" alignment="CENTER">
                        <VBox.margin>
                           <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
                        </VBox.margin>
                     </TextField>
                     <TextField fx:id="surname" alignment="CENTER">
                        <VBox.margin>
                           <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
                        </VBox.margin>
                     </TextField>
                  </VBox>
                  <VBox alignment="CENTER" spacing="25.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets/>
                     </HBox.margin>
                     <TextField fx:id="nickname">
                        <VBox.margin>
                           <Insets/>
                        </VBox.margin>
                     </TextField>
                     <ImageView fx:id="characterimage" fitHeight="200.0" fitWidth="200.0"
                                pickOnBounds="true" preserveRatio="true">
                        <viewport>
                           <Rectangle2D height="300.0" width="300.0"/>
                        </viewport>
                        <VBox.margin>
                           <Insets/>
                        </VBox.margin>
                     </ImageView>
                  </VBox>
                  <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
                     <TextField fx:id="age" alignment="CENTER">
                        <VBox.margin>
                           <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
                        </VBox.margin>
                     </TextField>
                     <TextField fx:id="gender" alignment="CENTER">
                        <VBox.margin>
                           <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
                        </VBox.margin>
                     </TextField>
                     <TextField fx:id="sex" alignment="CENTER">
                        <VBox.margin>
                           <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
                        </VBox.margin>
                     </TextField>
                     <TextField fx:id="sexuality" alignment="CENTER">
                        <VBox.margin>
                           <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
                        </VBox.margin>
                     </TextField>
                  </VBox>
               </HBox>
            </AnchorPane>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0"
                        SplitPane.resizableWithParent="false">
               <HBox alignment="CENTER" layoutX="281.0" layoutY="20.0" prefHeight="140.0"
                     prefWidth="762.4" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                     AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                  <padding>
                     <Insets left="100.0" right="100.0"/>
                  </padding>
                  <Button fx:id="create" contentDisplay="CENTER" mnemonicParsing="false"
                          prefHeight="70.0" prefWidth="70.0" text="Create" textAlignment="CENTER"
                          HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="30.0" right="30.0"/>
                     </HBox.margin>
                  </Button>
                  <Button fx:id="edit" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="70.0"
                          prefWidth="70.0" text="Edit" textAlignment="CENTER" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="30.0" right="30.0"/>
                     </HBox.margin>
                  </Button>
                  <Button fx:id="delete" contentDisplay="CENTER" mnemonicParsing="false"
                          prefHeight="70.0" prefWidth="70.0" text="Delete" textAlignment="CENTER"
                          HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="30.0" right="30.0"/>
                     </HBox.margin>
                  </Button>
                  <Button fx:id="random" alignment="CENTER" contentDisplay="CENTER"
                          mnemonicParsing="false" prefHeight="70.0" prefWidth="70.0" text="Random"
                          textAlignment="CENTER" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="30.0" right="30.0"/>
                     </HBox.margin>
                  </Button>
               </HBox>
            </AnchorPane>
         </SplitPane>
      </SplitPane>
   </AnchorPane>
</fx:root>
